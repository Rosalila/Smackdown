<section class="content-header">
  <h1>
    Historial
    <small>smackdowns recibidos</small>
  </h1>
</section>

<div class="box">
  <div class="box-body">
    <% @smackdowns.each do |smackdown| %>
      <%= render 'received_smackdown', :locals => {:smackdown=> smackdown} %>
    <% end %>
    <%= will_paginate @smackdowns %>
  </div><!-- /.box-body -->
</div><!-- /.box -->

<% if @smackdowns.count == 0 %>
  <p>Todavia no has completado ning√∫n Smackdown.</p>
<% end %>
