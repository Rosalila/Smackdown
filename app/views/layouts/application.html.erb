    <div class="wrapper">
      
      <header class="main-header">
        <!-- Logo -->
        <a href="index2.html" class="logo"><b>Smackdown.club</b></a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
          <!-- Navigation notificacions todo -->
        </nav>
      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Sidebar user panel -->
          <div class="user-panel">
            <!--
            <div class="pull-left image">
              <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image" />
            </div>
            <div class="pull-left info">
              <p>nya</p>

              <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
            -->
            <% if current_user %>
              <div class="pull-left info">
                <p>Bienvenido <%= current_user.name %></p>
              </div>
            <% end %>
          </div>
          <!-- sidebar menu: : style can be found in sidebar.less -->
          <ul class="sidebar-menu">
            <li class="header">Main menu</li>                  
              <% if current_user %>

                <% pending_respond_count = pendingRespondCount %>
                <% pending_judge_count = pendingJudgeCount %>
                <% wating_opponent_count = watingOpponentCount; %>
                <% wating_judges_count = getWatingJudge.count %>
            <li>
              <a href="/home/users">
                <i class="fa fa-th"></i> <span>Visitar</span> <small class="label pull-right bg-green">new</small>
              </a>
            </li>
             <li>
              <a href="/smackdown_creator/select_user">
                <i class="fa fa-th"></i> <span>Enviar smackdown</span> <small class="label pull-right bg-green">new</small>
              </a>
            </li>
            <li class="active treeview">
              <a href="#">
                <i class="fa fa-dashboard"></i> <span>Recibidos</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li class="active"><a href="/home/pending_respond_smackdowns"><i class="fa fa-circle-o"></i>Smackdowns (<%= pending_respond_count %>)</a></li>              
                <li class="active"><a href="/home/pending_judge_smackdowns"><i class="fa fa-circle-o"></i>Juicios (<%= pending_judge_count %>)</a></li>              
              </ul>
            </li>  
            <li class="active treeview">
              <a href="#">
                <i class="fa fa-dashboard"></i> <span>Esperando</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li class="active"><a href="/home/wating_opponent"><i class="fa fa-circle-o"></i>Oponente (<%= wating_opponent_count %>)</a></li>              
                <li class="active"><a href="/home/wating_judges"><i class="fa fa-circle-o"></i>Jueces (<%= wating_judges_count %>)</a></li>              
              </ul>
            </li>  
            <li class="active treeview">
              <a href="#">
                <i class="fa fa-dashboard"></i> <span>Historial</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li class="active"><a href="/home/history_sent_smackdowns"><i class="fa fa-circle-o"></i>Enviados</a></li>              
                <li class="active"><a href="/home/history_received_smackdowns"><i class="fa fa-circle-o"></i>Recibidos</a></li>              
                <li class="active"><a href="/home/history_judged_smackdowns"><i class="fa fa-circle-o"></i>Juzgados</a></li>              
              </ul>
            </li>  
             <li>
              <a href="/home/profile">
                <i class="fa fa-th"></i> <span>Mi perfil</span> <small class="label pull-right bg-green">new</small>
              </a>
            </li>
             <li>
              <a href="<%= signout_path %>">
                <i class="fa fa-th"></i> <span>Cerrar sesion</span> <small class="label pull-right bg-green">new</small>
              </a>
            </li>
            <% else %>
              <li>
                <a href="/auth/facebook">
                  <i class="fa fa-th"></i> <span>Iniciar sesion</span> <small class="label pull-right bg-green">new</small>
                </a>
              </li>
            <% end %>

