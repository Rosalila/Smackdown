    <tr>
      <th>User</th>
      <th>Community</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_in_communities.each do |user_in_community| %>
      <tr>
        <td><%= user_in_community.user_id %></td>
        <td><%= user_in_community.community_id %></td>
        <td><%= link_to 'Show', user_in_community %></td>
